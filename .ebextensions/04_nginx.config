files:
  "/etc/nginx/conf.d/gzip.conf":
    mode: "000644"
    owner: root
    group: root
    content: |
      gzip on;
      gzip_comp_level 9;
      gzip_types text/plain text/css text/js text/xml text/javascript application/javascript;
option_settings:
  aws:elasticbeanstalk:cloudwatch:logs:
    StreamLogs: true
    RetentionInDays: 7
